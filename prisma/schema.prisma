// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Student {
  id        String    @id
  name      String
  audio     String?
  grade     String
  section   String
  image     String?
  createdAt DateTime  @default(now())
  updatedAt DateTime? @updatedAt
}
